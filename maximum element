//#include<iostream>
#include<bits/stdc++.h>
using namespace std;

int main(){
	int n;
	cin>>n;
	
	int arr[n];
	for(int i=0;i<n;i++){
		cin>>arr[i];
	}
	
	//first approach
//	sort(arr,arr+n);
//	cout<<"max item is:  "<<arr[n-1];   time complexity=nlogn

//second approach  time complexity n

    int max=arr[0];
    for(int i=1;i<n;i++){
    	if(arr[i]>max){
    		max=arr[i];
		}
	}
	
	cout<<max<<"max";
	
}
